<%= form_with model: @house do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :price %>
  <%= f.number_field :price %>
  <%= f.label :picture %>
  <%= f.text_field :picture %>
  <%= f.submit %>
<% end %>
